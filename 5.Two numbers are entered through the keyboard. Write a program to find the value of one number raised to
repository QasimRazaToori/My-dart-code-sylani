import 'dart:io'; //Two numbers are entered through the keyboard. Write a program to find the value of one number raised to
import 'dart:math'; //This labrary have pow exponenetial function code

void main() {
  print("Please Enter value");
  int n = int.parse(stdin.readLineSync()!);
  print("Please Enter second value");
  int e = int.parse(stdin.readLineSync()!);
  var result = pow(n, e);
  print(result);
}
